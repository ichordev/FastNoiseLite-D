{
	"authors": [
		"Aya Partridge"
	],
	"description": "FastNoiseLite bindings (static)",
	"license": "MIT",
	"name": "fnl",
	
	"targetType": "staticLibrary",
	"configurations": [
		{
			"name": "double",
			"preBuildCommands": [
				"gcc -O2 -w -r -fPIE -o libFastNoiseLite.o fnl_double.c"
			],
		},
		{
			"name": "float",
			"preBuildCommands": [
				"gcc -O2 -w -r -fPIE -o libFastNoiseLite.o fnl_float.c"
			],
			"versions": ["FNL_Float"],
		},
	],
	"lflags": [
		"$PACKAGE_DIR/libFastNoiseLite.o"
	],
}
